{
    "name": "Cedar GoLinks",
    "version": "1.0.0",
    "description": "Hackathon Project",
    "manifest_version": 3,
    "author": "Brian Cheang, Ryan Yeh",
    "action":{
        "default_popup": "index.html",
        "default_icon": {
            "16": "images/favicon-16x16.png",
            "32": "images/favicon-32x32.png"
        },
        "default_title": "Cedar GoLinks"
    },
    "permissions": [
        "webNavigation",
        "tabs",
        "activeTab",
        "clipboardWrite"
    ],
    "host_permissions": [
        "http://cheangmbrian.pythonanywhere.com/*"
    ],
    "background": {
        "service_worker": "service_worker.js",
        "type": "module"
    }
}