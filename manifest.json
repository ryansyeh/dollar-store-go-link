{
  "manifest_version": 3,
  "name": "Hello Extensions!",
  "description": "Base Level Extension",
  "version": "1.0",
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": "images/hello_extensions.png"
  },
  "permissions": [
    "tabs"
  ],
  "host_permissions": [
    "https://developer.chrome.com/*"
  ]
}